#include <stdio.h>

int main()
{
    int v; 
	int p; 
	int l; 
	int t; 
	int t_ter; 
	int n; 
	int i;
	
    v=p*l*t;
    t_ter=41;
    i=1;
    printf("kotak sejumlah n dari 1 sampai 100: ");
    scanf("%d",&n);
    while(i<=n){
        printf("masukkan p l t dari 1 sampai 100: ");
        scanf("%d %d %d",&p,&l,&t);
        i=++i;
		
        if(t<t_ter){
            v=p*l*t;
            printf("%d\n",v);
		}
			else {
				printf ("tinggi kotak tidak memenuhi syarat\n");
			}
			
        }
		
return 0;
}

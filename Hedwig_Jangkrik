#include <stdio.h>

int main()
{
int X1,V1,X2,V2;
 
printf("masukkan X1 V1 X2 V2 :");
scanf("%d %d %d %d",&X1,&V1,&X2,&V2);

if (X1<X2&&1<=V1<=100&&1<=V2<=1000) {   
do {
X1=X1+V1;
X2=X2+V2;
} while (X1!=X2);

if (X1>=0&&X1<=1000) printf ("YES");
else printf ("NO");
}

else printf ("jangkrik 2 harus start di depan jangkrik 1");

return 0;
}
